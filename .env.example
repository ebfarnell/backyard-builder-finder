# Required for backend to start
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# External services
REGRID_API_KEY=your_regrid_api_key
NAIP_TEMPLATE_URL=https://naip-analytic.s3.amazonaws.com/naip/2022/{z}/{x}/{y}.tif

# Geocoding services
VITE_MAPBOX_API_KEY=your_mapbox_public_token

# LLM providers (at least one required for edge case analysis)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
LLM_ANTHROPIC_ENABLED=false

# Optional - Computer Vision Service
CV_SERVICE_URL=http://localhost:8000

# Optional - Observability
SENTRY_DSN_API=your_sentry_dsn

# Optional - Rate limiting & caps (defaults shown)
MAX_PARCELS_PER_SEARCH=10000
LLM_BUDGET_PER_SEARCH=1.0
SEARCH_RATE_LIMIT_PER_HOUR=10

# Development
NODE_ENV=development
PORT=3001
VITE_APP_URL=http://localhost:5173